    "prettier-check": "prettier --list-different \"./src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json src/*.ts"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.0",
